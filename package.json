{
  "name": "ksc-node-api-starter",
  "description": "The starter node API server for Kanzelmeyer Software Company",
  "version": "0.0.1",
  "engines": {
    "node": "5.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanzelm3/ksc-node-api-starter.git"
  },
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/server.js",
    "test": "lab -t 100 -S -T ./node_modules/lab-babel -I __core-js_shared__ ./test/",
    "cover": "lab -T ./node_modules/lab-babel -I __core-js_shared__ --sourcemaps -r html -o ./coverage/index.html ./test/ && open ./coverage/index.html && exit 0",
    "build": "babel src -d dist",
    "serve": "nodemon src/server.js --exec babel-node"
  },
  "keywords": [
    "hapijs",
    "es6",
    "api",
    "server"
  ],
  "author": "Joel Kanzelmeyer <kanzelm3@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "code": "^2.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "lab": "^8.0.0",
    "lab-babel": "^1.1.1",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "boom": "^3.1.2",
    "firebase": "^2.4.0",
    "firebase-token-generator": "^2.0.0",
    "good": "^6.1.2",
    "hapi": "^11.1.2",
    "hapi-swagger": "^4.2.0",
    "inert": "^3.2.0",
    "vision": "^4.0.1"
  },
  "bugs": {
    "url": "https://github.com/abiee/es6-hapijs/issues"
  },
  "homepage": "https://github.com/abiee/es6-hapijs#readme",
  "directories": {
    "test": "test"
  }
}
